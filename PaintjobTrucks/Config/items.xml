			<property name="EconomicValue" value="30000"/>
			<property class="Action1">
				<property name="Vehicle" value="pjtTest"/>
				<property name="VehicleSize" value="2.5, 2.2, 6.5"/>
			</property>
			<effect_group name="pjtTestPlaceable" tiered="false">
